[color]
  branch = auto
	ui = auto
[color "branch"]
	current = yellow bold
	local = white
	remote = yellow dim
[color "diff"]
	meta = white bold
	frag = magenta bold
	old = red bold
	new = green bold
	whitespace = red reverse
[color "status"]
	added = cyan bold
  branch = yellow bold
	changed = red bold
	untracked = magenta bold
[alias]
  df = diff
  st = status
  cm = commit
  co = checkout
  br = branch
  last = log -n 1
[credential]
	helper = osxkeychain
