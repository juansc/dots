[color]
  branch = auto
	ui = true
[color "branch"]
	current = yellow bold
	local = white
	remote = yellow dim
[color "diff"]
	meta = yellow
	frag = magenta bold
	old = red bold
	new = green bold
	whitespace = red reverse
	commit = yellow bold
[color "status"]
	added = cyan bold
  branch = yellow bold
	changed = red bold
	untracked = magenta bold
[alias]
  cr = checkout -b
  df = diff
  st = status
  cm = commit
  co = checkout
  br = branch
  last = log --oneline -n 1
  minlog = log --oneline
  statlog = log --stat
  taglist = tag -l --sort=-v:refname
  tagl = tag -l --sort=-v:refname
[credential]
	helper = osxkeychain
[user]
	name = Juan
	email = juanscarrillo23@gmail.com
[core]
	excludesfile = /Users/juansc/.gitignore_global
	pager = diff-so-fancy | less --tabs=4 -RFX
[push]
	followTags = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22

