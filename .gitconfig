[color]
	branch = auto
	ui = true
[color "branch"]
	current = yellow bold
	local = white
	remote = yellow dim
[color "diff"]
	commit = yellow bold
	frag = magenta bold
	meta = yellow
	new = green bold
	old = red bold
	whitespace = red reverse
[color "status"]
	added = cyan bold
	branch = yellow bold
	changed = red bold
	untracked = magenta bold
[alias]
	br = branch
	cm = commit
	co = checkout
	cr = checkout -b
	df = diff
	last = log --oneline -n 1
	minlog = log --oneline
	pn = push origin -u HEAD
	pu = push
	st = status
	statlog = log --stat
	taglist = tag -l --sort=-v:refname
	tagl = tag -l --sort=-v:refname
[credential]
	helper = osxkeychain
[user]
	email = juanscarrillo23@gmail.com
	name = Juan
[core]
	excludesfile = /Users/juansc/.gitignore_global
	pager = diff-so-fancy | less --tabs=4 -RFX
[push]
	followTags = true
[color "diff-highlight"]
	newNormal = green bold
	newHighlight = green bold 22
	oldNormal = red bold
	oldHighlight = red bold 52
	
